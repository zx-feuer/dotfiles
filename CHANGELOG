# -------------------------------
## dotfiles - $HOME = ~/.files  |
# -------------------------------

git init --bare $HOME/.files
alias dotfiles='/usr/bin/git --git-dir=$HOME/.files/ --work-tree=$HOME'
dotfiles config status.showUntrackedFiles no

